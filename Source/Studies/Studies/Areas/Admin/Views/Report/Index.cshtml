@model dynamic

@{
    ViewBag.Title = "Report";
    Layout =  "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}


<div class="page-header page-header-default">
    <div class="page-header-content" style="background-color: #fcfcfc;">
        <div class="page-title" style="padding: 20px 0;">
            <h4>
                <a href="javascript:void(0);" onclick="history.go(-1);"><i class="icon-arrow-left52 position-left"></i></a>
                <span class="text-semibold">Báo cáo</span> - Báo cáo chi phí
            </h4>
        </div>
    </div>
</div>
<div class="div-h100 div-top69">
    <div class="panel-body div-h100-body">
        <div class="two-columns form-group">
            <div class="left-full">
                <div class="row">
                    <div class="col-md-offset-2 col-md-3 form-group">
                        <label>Giờ vào</label>
                        <div class="input-group">
                            <div class="input-group">
                                <span class="input-group-addon"><i class="icon-alarm"></i></span>
                                <input type="text" class="form-control pickadate" id="dateStart" >
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 form-group">
                        <label>Giờ kết thúc</label>
                        <div class="input-group">
                            <div class="input-group">
                                <span class="input-group-addon"><i class="icon-alarm"></i></span>
                                <input type="text" class="form-control pickadate" id="dateEnd" >
                            </div>
                        </div>
                    </div>
                    <div class="col-md-1 form-group">
                        <label>Loại</label>
                        <div class="input-group">
                            <select class="form-control select-search w-100" id="type">
                                <option value="" selected="selected">Chọn</option>
                                <option value="DAY" >Ngày</option>
                                <option value="MONTH">Tháng</option>
                                <option value="YEAR">Năm</option>
                            </select>
                        </div>
                    </div>
                    
                    <div class="text-center col-md-1" style="margin-top: 28px">
                                        <button type="button" class="btn btn-primary btnSearch">Tìm kiếm</button>
                                        </div>
                </div>
                
            </div>
            
        </div>
        <div class="div-h100 div-top105">
            <div class="div-h100 div-top56">
                <div class="data-table div-h100-body">
                    @{ Html.RenderAction("_Index");}
                </div>
            </div>
        </div>
    </div>
</div>